<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="department"
		class="com.capg.springcore.beans.DepartmentBean">
		<property name="deptId" value="111"></property>
		<property name="deptName" value="department"></property>
	</bean>
	<bean id="hr"
		class="com.capg.springcore.beans.DepartmentBean" primary= "true">
		<property name="deptId" value="112"></property>
		<property name="deptName" value="HR"></property>
	</bean>
	</beans>
