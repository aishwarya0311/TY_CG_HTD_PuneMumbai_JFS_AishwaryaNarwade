<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <!-- Configuration for sports -->
        <bean id ="sports" class="com.capg.springcore.annotations.beans.Sports"> 
        <property name="name" value ="Cricket"></property>
        <property name="totalplayers" value="11"></property>
        <property name="teamInfo">
        <map>
        <entry key ="India "  value ="Kohli"></entry>
        <entry key ="New Zealand" value ="Kane WillamSon"></entry>
        <entry key ="Sri Lanka" value ="Dimuth"></entry>
        </map>
        </property>
        </bean>
	</beans>